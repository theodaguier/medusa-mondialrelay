	"license": "MIT",
	"keywords": [
		"medusa-v2",
		"medusa-plugin-shipping",
		"medusa-plugin-integration",
		"medusa-fulfillment",
		"mondial-relay",
		"shipping"
	],
	"main": ".medusa/server/providers/mondialrelay/index.js",
	"exports": {
		"./package.json": "./package.json",
		"./providers/*": "./.medusa/server/providers/*/index.js",
		"./.medusa/admin": {
			"import": "./.medusa/server/src/admin/index.mjs",
			"require": "./.medusa/server/src/admin/index.js"
		},
		"./*": "./.medusa/server/*.js"
	},
	"files": [
		".medusa",
		"!.medusa/server/src/**/__tests__",
		"!.medusa/server/src/**/__fixtures__",
		"!.medusa/server/src/**/__mocks__"
	],
	"scripts": {
		"build": "medusa plugin:build",
		"watch": "medusa plugin:develop",
		"publish:local": "medusa plugin:publish",
		"test": "jest --passWithNoTests src",
		"prepublishOnly": "medusa plugin:build"
	},
	"dependencies": {
		"@frontboi/mondial-relay": "^1.2.49",
		"@medusajs/icons": "^2.12.1",
